      <?php
      $doc = simplexml_load_string('<?xml version="1.0"?>
      <namelist>
       <name>
        <first>helly</first>
       </name>
       <name>
        <first>sterling</first>
       </name>
      </namelist>');
      var_dump($doc->name[0]->first);
      
