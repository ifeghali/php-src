#! /bin/sh

prefix='@PREFIX@'
includes='-I@PHPINCLUDEDIR@ -I@PHPINCLUDEDIR@/main -I@PHPINCLUDEDIR@/Zend -I@PHPINCLUDEDIR@/TSRM'
extension_dir='@EXTENSION_DIR@'

case "$1" in
--prefix)
	echo $prefix;;
--includes)
	echo $includes;;
--extension-dir)
	echo $extension_dir;;
*)
	echo "Usage: $0 [--prefix|--includes|--extension-dir]"
	exit 1;;
esac

exit 0
