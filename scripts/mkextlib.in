#!/bin/sh

outfile=$1; shift
extensions=$@

ext_libs=""
for dir in $extensions; do
    archive="$dir/libphpext_$dir.a"
    if test ! -r "$archive"; then
	echo `basename $0`": could not open $archive" >&2
	exit 1
    fi
    ext_libs="$ext_libs $archive"
done

(set -x; $top_srcdir/scripts/armerge $outfile $ext_libs)
exit 0
